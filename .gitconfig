[user]
  name = Eliezer Tal√≥n
  email = elitalon@gmail.com

[alias]
  last = "!f() { git log -`if [ -z $1 ]; then echo 1; else echo $1; fi` HEAD;  }; f"
  co = checkout
  go = checkout -b
  ci = commit
  st = status
  plog = log --oneline --decorate
  sm = submodule
  br = branch -av
  brd = branch -d
  tags = tag -ln
  remotes = remote -v
  contributors = shortlog -s -n
  lsd = ls-files --deleted
  prune-remote = git remote | xargs git remote prune

[apply]
  whitespace = fix

[core]
  excludesfile = ~/.gitignore
  editor = "mate -w"
  whitespace = space-before-tab,trailing-space

[color]
  branch = auto
  interactive = auto
  diff = auto
  status = auto

[merge]
  log = true

[credential]
  helper = osxkeychain

[github]
  user = elitalon

[push]
  default = upstream
